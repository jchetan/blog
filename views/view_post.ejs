<%- include('layouts/header_new'); -%>  
<body id="body">
<!--
Fixed Navigation
==================================== -->
<%- include('layouts/navbar_new'); -%>

<section class="blog section" id="blog">
    <div class="container">
  
        <div class="row">                
            <!-- single blog post -->
                <article class="col" >
                    <div class="post-item">
                        <div class="post-title">
                            <h3 class="mt-0"><a href=""><%= blogpost.title %></a></h3>
                        </div>
                        <div class="post-meta">
                            <ul class="list-inline">
                                <li class="list-inline-item">
                                    <span>Created: </span> <a href="" class=""><%= blogpost.date_posted %></a>        
                                </li>
                                <li class="list-inline-item">
                                    <span>Updated: </span> <span> <%= blogpost.date_updated %></span>        
                                </li>
                            </ul>
                        </div>
                        <div class="post-content">
                            <p><%= blogpost.body %></p>
                        </div>
                    </div>
                </article>
            <!-- end single blog post -->
            
        </div> <!-- end row -->
    </div> <!-- end container -->
</section> <!-- end section -->


       
<%- include('layouts/footer_new'); -%>
<%- include('layouts/scripts_new'); -%>
</body>
</html>